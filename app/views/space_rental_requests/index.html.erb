<p id="notice"><%= notice %></p>

<h1>Listing Space Rental Requests</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @space_rental_requests.each do |space_rental_request| %>
      <tr>
        <td><%= space_rental_request.location %></td>
        <td><%= space_rental_request.start %></td>
        <td><%= space_rental_request.end %></td>
        <td><%= link_to 'Show', space_rental_request %></td>
        <td><%= link_to 'Edit', edit_space_rental_request_path(space_rental_request) %></td>
        <td><%= link_to 'Destroy', space_rental_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space rental request', new_space_rental_request_path %>
